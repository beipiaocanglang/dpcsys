<?xml version="1.0" encoding="UTF-8" ?>
<sqlList tableName="park_config_data">
    <primaryKey dbPrimaryKey="id" classPrimaryKey="id" type="java.lang.Long"/>
    <tableFiledList>
        <filed dbFiledName="store_id" classFiledName="storeId" type="java.lang.String"/>
        <filed dbFiledName="card_type" classFiledName="cardTypeDesc" type="java.lang.Integer"/>
        <filed dbFiledName="card_typeDesc" classFiledName="cardTypeDesc" type="java.lang.String"/>
        <filed dbFiledName="park_type" classFiledName="parkType" type="java.lang.Integer"/>
        <filed dbFiledName="total_time" classFiledName="totalTime" type="java.lang.Long"/>
        <filed dbFiledName="valid_start_time" classFiledName="validStartTime" type="java.util.Date"/>
        <filed dbFiledName="valid_end_time" classFiledName="insTimvalidEndTimee" type="java.util.Date"/>
        <filed dbFiledName="create_time" classFiledName="createTime" type="java.sql.Timestamp"/>
        <filed dbFiledName="update_time" classFiledName="updateTime" type="java.sql.Timestamp"/>
        <filed dbFiledName="select_scheme" classFiledName="selectScheme" type="java.lang.Integer"/>
        <filed dbFiledName="status_flag" classFiledName="statusFlag" type="java.lang.Integer"/>
    </tableFiledList>

    <sqlElement key="findList">
        <![CDATA[
			SELECT
                id, store_id, card_type, card_typeDesc, park_type, total_time, valid_start_time, valid_end_time, create_time, status_flag, select_scheme, update_time
            FROM
                park_config_data
            WHERE
                1 = 1
                #if("$!storeId" != "") and store_id = :storeId #end
			ORDER BY
                valid_start_time DESC ,
                valid_end_time DESC
	    ]]>
    </sqlElement>
</sqlList>